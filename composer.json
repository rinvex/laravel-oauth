{
    "name": "rinvex/laravel-oauth",
    "description": "Rinvex OAuth provides OAuth2 server support to Laravel.",
    "type": "library",
    "keywords": [
        "laravel",
        "rinvex",
        "oauth",
        "api"
    ],
    "license": "MIT",
    "homepage": "https://rinvex.com",
    "support": {
        "email": "help@rinvex.com",
        "issues": "https://github.com/rinvex/laravel-oauth/issues",
        "source": "https://github.com/rinvex/laravel-oauth",
        "docs": "https://github.com/rinvex/laravel-oauth/blob/master/README.md"
    },
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        },
        {
            "name": "Rinvex LLC",
            "homepage": "https://rinvex.com",
            "email": "help@rinvex.com"
        },
        {
            "name": "Abdelrahman Omran",
            "homepage": "https://omranic.com",
            "email": "me@omranic.com",
            "role": "Project Lead"
        },
        {
            "name": "The Generous Laravel Community",
            "homepage": "https://github.com/rinvex/laravel-oauth/contributors"
        }
    ],
    "require": {
        "php": "^8.1.0",
        "ext-json": "*",
        "firebase/php-jwt": "^6.4.0",
        "illuminate/auth": "^10.0.0 || ^11.0.0",
        "illuminate/console": "^10.0.0 || ^11.0.0",
        "illuminate/contracts": "^10.0.0 || ^11.0.0",
        "illuminate/cookie": "^10.0.0 || ^11.0.0",
        "illuminate/database": "^10.0.0 || ^11.0.0",
        "illuminate/http": "^10.0.0 || ^11.0.0",
        "illuminate/support": "^10.0.0 || ^11.0.0",
        "league/oauth2-server": "^8.5.0",
        "lcobucci/jwt": "^5.0.0",
        "nyholm/psr7": "^1.7.0",
        "phpseclib/phpseclib": "^3.0.0",
        "psr/http-message": "^2.0.0",
        "rinvex/laravel-support": "^7.0.0",
        "rinvex/tmp-josephsilber-bouncer": "^1.0.0",
        "symfony/http-foundation": "^6.2.0",
        "symfony/psr-http-message-bridge": "^2.2.0",
        "symfony/console": "^6.2.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.6.0",
        "orchestra/testbench": "^8.0.0",
        "phpunit/phpunit": "^10.1.0"
    },
    "autoload": {
        "psr-4": {
            "Rinvex\\Oauth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rinvex\\Oauth\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rinvex\\Oauth\\Providers\\OAuthServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
